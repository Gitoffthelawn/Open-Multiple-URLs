{
  "name": "open-multiple-urls",
  "version": "1.5.0",
  "description": "Browser extension that opens a list of URLs",
  "scripts": {
    "dev": "parcel watch src/*.html --out-dir dev-out --no-hmr",
    "build": "parcel build src/*.html",
    "build-run-all": "npm run format && npm run lint && npm run test && npm run build",
    "format": "npx prettier --write .",
    "lint": "npx eslint . --fix --ext .ts,.js",
    "test": "npx jest --collect-coverage",
    "zip": "zip -FSrj open-multiple-urls-$npm_package_version.zip dist"
  },
  "keywords": [],
  "author": "https://github.com/htrinter/",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "eslint": "^7.12.0",
    "jest": "^26.6.1",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-clean-dist": "0.0.6",
    "parcel-plugin-static-files-copy": "^2.5.0",
    "prettier": "2.1.2",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.5",
    "webextension-polyfill-ts": "^0.21.0"
  },
  "dependencies": {},
  "staticFiles": {
    "staticPath": [
      "src/static"
    ],
    "watcherGlob": "**"
  }
}

{
  "name": "Open Multiple URLs",
  "version": "1.5.5",
  "manifest_version": 3,
  "description": "Opens a list of URLs",
  "homepage_url": "https://github.com/htrinter/Open-Multiple-URLs",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_icon": "icon19.png",
    "default_popup": "browseraction.html"
  },
  "permissions": ["storage"],
  "incognito": "split"
}
